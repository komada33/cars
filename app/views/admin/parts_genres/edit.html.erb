<div class="pgenre-edit">

  <h2>PartsGenre Edit</h2>


  <div class="pgenre-form-edit">
    <%= form_with model: @part, url: admin_parts_genre_path(@part.id), method: :patch, local:true do |f| %>
      <%= f.text_field :pname %>
      <%= f.submit "編集" %>
    <% end %>
  </div>

  <div class="pgenre-delete">
    <%= link_to "削除", admin_parts_genre_path(@part.id), method: :delete, "data-confirm" => "本当に削除しますか？" %>
  </div>

</div>

